{
	"codeLens.enable": true,
	"list.normalMappings": {
		"<C-l>": "command:CocNext",
		"<C-h>": "command:CocPrev"
	},
	"list.insertMappings": {
		"<C-l>": "command:CocNext",
		"<C-h>": "command:CocPrev"
	},
	//"suggest.enablePreselect": true,  // Select first item in menu, but don't insert it

	"go.goplsOptions":{
		// This ensures that function completion includes the parameters in the completion snippet.
		"usePlaceholders":true ,
		"gofumpt": true,
		"analyses": {
			"nilness": true,
			"shadow": true,
			"unusedparams": true,
			"unusedwrite": true,
			"unusedvariable": true
		},
		"verboseOutput" : false,
	},
	"codeLens.position": "eol",
	"rust-analyzer.lens.enable": true,
	"rust-analyzer.cargo.buildScripts.enable": true,
	"rust-analyzer.cargo.loadOutDirsFromCheck": true,
	"rust-analyzer.procMacro.attributes.enable": true,
	"tree.key.toggleSelection": "s", // By default this is set to space but that interferes with all my leader commands so set to s
	"pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
	"typescript.format.enable": false,
	"javascript.format.enable": false
}
